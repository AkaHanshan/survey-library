{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "survey-angular-ui": {
      "projectType": "library",
      "root": "build-scripts/survey-angular-ui",
      "sourceRoot": "src/angular",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "build-scripts/survey-angular-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "build-scripts/survey-angular-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "build-scripts/survey-angular-ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "build-scripts/survey-angular-ui/src/test.ts",
            "tsConfig": "build-scripts/survey-angular-ui/tsconfig.spec.json",
            "karmaConfig": "build-scripts/survey-angular-ui/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "survey-angular-ui"
}

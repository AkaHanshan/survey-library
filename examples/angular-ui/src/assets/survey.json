{
  "showProgressBar": "top",
  "title": "Survey New Design Test",
  "logo": "https://surveyjs.io/Content/Images/examples/image-picker/lion.jpg",
  "logoPosition": "left",
  "pages": [
    {
      "name": "page1",
      "title": "Page 1 Title",
      "description": "This is the first page of test survey shown via survey-angular-ui module.",
      "questions": [
        {
          "name": "radiogroup",
          "type": "radiogroup",
          "title": "Choose an item",
          "choices": [
            "item1",
            "item2",
            "item3"
          ]
        },
        {
          "name": "name",
          "type": "text",
          "title": "Text",
          "enableIf": "{radiogroup} == 'item1'",
          "placeHolder": "Jon Snow"
        },
        {
          "name": "birthdate",
          "type": "text",
          "inputType": "date",
          "title": "Text Date"
        },
        {
          "name": "color",
          "type": "text",
          "inputType": "color",
          "title": "Text Color"
        },
        {
          "name": "email",
          "type": "text",
          "visibleIf": "{radiogroup} == 'item1'",
          "inputType": "email",
          "title": "Text Email",
          "placeHolder": "jon.snow@nightwatch.org",
          "validators": [
            {
              "type": "email"
            }
          ]
        }
      ]
    },
    {
      "name": "page2",
      "title": "Page 2",
      "questions": [
        {
          "name": "checkbox",
          "type": "checkbox",
          "title": "Choose item(s)",
          "choices": [
            "item1",
            "item2",
            "item3"
          ]
        },
        {
          "name": "surname",
          "type": "text",
          "title": "Surname",
          "placeHolder": "Snow"
        },
        {
          "name": "signature",
          "type": "signaturepad",
          "title": "Sign here"
        }
      ]
    }
  ]
}